<% @send_unit_values.each do |send_unit_value| %>
      <tr>
        <td><%= send_unit_value.reason %></td>
        <td><%= send_unit_value.customer %></td>
        <td><%= send_unit_value.amount %></td>
        <td><%= send_unit_value.agent %></td>
        <td>
					<div class="action-buttons">
					<%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'),  edit_send_unit_value_path(send_unit_value), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), send_unit_value_delete_path(send_unit_value), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
      
      </tr>
    <% end %>