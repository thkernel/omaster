<% @agents.each do |agent| %>
      <tr>
        <td><%= agent.first_name %></td>
        <td><%= agent.last_name %></td>
        <td><%= agent.gender %></td>
        <td><%= agent.address %></td>
        <td><%= agent.phone %></td>
        <td><%= agent.city %></td>
        <td><%= agent.email %></td>
        <td>
	    <div class="action-buttons">
			<%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'),  edit_agent_path(agent), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), agent_delete_path(agent), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
      </tr>
    <% end %>